  "main": "dist/apps/functions/main.js",
  "browser": {
    "apollo-server": false,
    "crypto": false,
    "fs": false,
    "express": false,
    "http": false,
    "https": false,
    "net": false,
    "os": false,
    "path": false,
    "stream": false,
    "zlib": false
