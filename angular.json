{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "ng-scrappy",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "ng-scrappy": {
      "projectType": "application",
      "root": "apps/ng-scrappy",
      "sourceRoot": "apps/ng-scrappy/src",
      "prefix": "ng-scrappy",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng-scrappy",
            "index": "apps/ng-scrappy/src/index.html",
            "main": "apps/ng-scrappy/src/main.ts",
            "polyfills": "apps/ng-scrappy/src/polyfills.ts",
            "tsConfig": "apps/ng-scrappy/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-scrappy/src/favicon.ico",
              "apps/ng-scrappy/src/assets"
            ],
            "styles": [
              "apps/ng-scrappy/src/styles.scss",
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-scrappy/src/environments/environment.ts",
                  "with": "apps/ng-scrappy/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-scrappy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-scrappy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-scrappy:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-scrappy/src/**/*.ts",
              "apps/ng-scrappy/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ng-scrappy"
          ],
          "options": {
            "jestConfig": "apps/ng-scrappy/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "ng-scrappy-e2e": {
      "root": "apps/ng-scrappy-e2e",
      "sourceRoot": "apps/ng-scrappy-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-scrappy-e2e/cypress.json",
            "tsConfig": "apps/ng-scrappy-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-scrappy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-scrappy:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-scrappy-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "functions": {
      "root": "apps/functions",
      "sourceRoot": "apps/functions/src",
      "projectType": "application",
      "prefix": "functions",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/functions",
            "main": "apps/functions/src/main.ts",
            "tsConfig": "apps/functions/tsconfig.app.json",
            "assets": [
              "apps/functions/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/functions/src/environments/environment.ts",
                  "with": "apps/functions/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "functions:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/functions/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/functions"
          ],
          "options": {
            "jestConfig": "apps/functions/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-scrappy": {
      "root": "apps/react-scrappy",
      "sourceRoot": "apps/react-scrappy/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/react-scrappy",
            "index": "apps/react-scrappy/src/index.html",
            "main": "apps/react-scrappy/src/main.tsx",
            "polyfills": "apps/react-scrappy/src/polyfills.ts",
            "tsConfig": "apps/react-scrappy/tsconfig.app.json",
            "assets": [
              "apps/react-scrappy/src/favicon.ico",
              "apps/react-scrappy/src/assets",
              "apps/react-scrappy/src/manifest.webmanifest",
              "apps/react-scrappy/src/serviceWorker.js"
            ],
            "styles": [
              "apps/react-scrappy/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-scrappy/src/environments/environment.ts",
                  "with": "apps/react-scrappy/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-scrappy:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-scrappy:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-scrappy/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/react-scrappy"
          ],
          "options": {
            "jestConfig": "apps/react-scrappy/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-scrappy-e2e": {
      "root": "apps/react-scrappy-e2e",
      "sourceRoot": "apps/react-scrappy-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-scrappy-e2e/cypress.json",
            "tsConfig": "apps/react-scrappy-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-scrappy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-scrappy:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-scrappy-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/models/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/models"
          ],
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
